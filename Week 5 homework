#import sys
def add_them_all(filename):
    sum = 0
    contents= open(filename, 'r')
    contentslist = filename.readline()
    splitter_list = contents.split()
    for x in splitterlist:
        sum= sum + int(x)
    return sum 
    
        
    
   
if __name__ == "__main__":
    fname = sys.argv[1]
    print(f"Processing {fname}")
    print(add_them_all(fname))
